# Discord Bot Token
# Dapatkan dari Discord Developer Portal: https://discord.com/developers/applications
DISCORD_BOT_TOKEN=YOUR_BOT_TOKEN_HERE

# Discord OAuth2 (untuk Dashboard)
# Dapatkan dari Discord Developer Portal > OAuth2 > General
DISCORD_CLIENT_ID=YOUR_CLIENT_ID_HERE
DISCORD_CLIENT_SECRET=YOUR_CLIENT_SECRET_HERE
DISCORD_REDIRECT_URI=http://localhost:5000/callback

# Dashboard Secret Key (optional, akan digenerate otomatis jika kosong)
DASHBOARD_SECRET_KEY=

# Dashboard Base URL (untuk generate full URL ke file yang diupload)
# Ubah sesuai dengan URL dashboard Anda yang bisa diakses oleh bot
DASHBOARD_BASE_URL=http://localhost:5001
